<h1><%= @date %></h1>
<% while @open <= @close do %>
  
    <div><%="#{@open}-#{@open+3600}"%>
  <% if not_reserved? @date, @open.strftime("%H:%M")%>
    <%= form_with do |f| %>
      <%= f.hidden_field  :start_time, value:@open.strftime("%H:%M")%>
      <%= f.hidden_field  :end_time, value:(@open+3600).strftime("%H:%M")%>
      <%= f.submit "Reserve" %>
    <% end %>
  <% end %>
    </div>
    <%@open+= 3600%>
<% end %>
